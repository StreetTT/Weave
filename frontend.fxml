<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FrontendController">
   <children>
      <BorderPane layoutX="-1.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                      <MenuItem mnemonicParsing="false" text="Create new project" />
                      <MenuItem mnemonicParsing="false" text="Add new python file" />
                      <MenuItem mnemonicParsing="false" text="Save project" />
                      <MenuItem mnemonicParsing="false" text="Save project as" />
                      <MenuItem mnemonicParsing="false" text="Recent projects" />
                      <MenuItem mnemonicParsing="false" text="Export current project" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                      <MenuItem mnemonicParsing="false" text="Find python file" />
                      <MenuItem mnemonicParsing="false" text="Copy python file" />
                      <MenuItem mnemonicParsing="false" text="Paste python file" />
                      <MenuItem mnemonicParsing="false" text="Select all files" />
                      <MenuItem mnemonicParsing="false" text="Move selected file to" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Run">
                  <items>
                      <MenuItem mnemonicParsing="false" text="Run all" />
                      <MenuItem mnemonicParsing="false" text="Run Selected" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <left>
            <HBox fx:id="processContainer" alignment="CENTER_LEFT" fillHeight="false" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER_LEFT">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <children>
                  <Region fx:id="spacer" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="12.0" prefWidth="9.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></Region>
                  <Button fx:id="newBlockButton" mnemonicParsing="false" onAction="#addBlock" text="Add Block" />
               </children>
            </HBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
